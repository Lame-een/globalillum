ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

.PHONY: clean prebuild prelink

ifeq ($(config),release)
  RESCOMP = windres
  TARGETDIR = ../bin/x64/Release
  TARGET = $(TARGETDIR)/GIrt.exe
  OBJDIR = ../bin/intermediates/Release
  PCH = src/pch.h
  GCH = $(OBJDIR)/$(notdir $(PCH)).gch
  DEFINES += -DGLM_ENABLE_EXPERIMENTAL -D_CRT_SECURE_NO_WARNINGS -D_USE_MATH_DEFINES -DNDEBUG -D_CONSOLE
  INCLUDES += -Ivendor -Isrc -Ivendor/glm -Ivendor/nanoflann-1.4.3
  FORCE_INCLUDE += -include $(OBJDIR)/$(notdir $(PCH))
  ALL_CPPFLAGS += $(CPPFLAGS) -MD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -fopenmp -O2
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -fopenmp -O2 -std=c++17
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS +=
  LDDEPS +=
  ALL_LDFLAGS += $(LDFLAGS) -L/usr/lib64 -m64 -s -fopenmp
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: prebuild prelink $(TARGET)
	@:

endif

